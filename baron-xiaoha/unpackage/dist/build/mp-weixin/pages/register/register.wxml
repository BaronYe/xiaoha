<view class="content"><view class="logo"><image class="logo-image" src="https://vkceyugu.cdn.bspapp.com/VKCEYUGU-baron-xiaoha/a81c29e0-a7a2-11ea-b244-a9f5e5565f30.png"></image></view><view class="main"><view class="title"></view><view class="inputs"><view class="inputs-div"><input class="input" type="text" placeholder="请输入用户名" placeholder-style="color:#aaa;font-weight:400;" data-event-opts="{{[['blur',[['isname',['$event']]]],['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindblur="__e" bindinput="__e"/><view hidden="{{!(employ)}}" class="employ">已占用</view><image hidden="{{!(isuser)}}" class="ok" src="../../static/images/register/right.png"></image></view><view class="inputs-div"><input class="input" type="{{type}}" placeholder="请设置密码" placeholder-style="color:#aaa;font-weight:400;" data-event-opts="{{[['blur',[['check']]],['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindblur="__e" bindinput="__e"/><view hidden="{{!(employ)}}" class="employ">已占用</view><image class="show" src="{{showurl}}" data-event-opts="{{[['tap',[['showon']]]]}}" bindtap="__e"></image></view><view class="inputs-div"><input class="input" type="{{type}}" placeholder="请确认密码" placeholder-style="color:#aaa;font-weight:400;" data-event-opts="{{[['blur',[['checknumber']]],['input',[['__set_model',['','check_password','$event',[]]]]]]}}" value="{{check_password}}" bindblur="__e" bindinput="__e"/><view hidden="{{!(employ)}}" class="employ">已占用</view><text hidden="{{!(checkshow)}}" class="checknumber">请输入6-20位字母数字组合且保证两次密码一致</text><image class="show" src="{{showurl}}" data-event-opts="{{[['tap',[['showon']]]]}}" bindtap="__e"></image></view></view></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit" bindtap="__e">注册</view></view>