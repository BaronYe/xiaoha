<view class="content"><view class="top-bar"><view data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" class="top-bar-left" bindtap="__e"><image class="back" src="../../static/images/register/back.png"></image></view></view><view class="logo"><image class="logo-image" src="../../static/images/index/logo.png"></image></view><view class="main"><view class="title"></view><view class="inputs"><view class="inputs-div"><input class="input" type="text" placeholder="请输入用户名" placeholder-style="color:#aaa;font-weight:400;" data-event-opts="{{[['blur',[['isname',['$event']]]],['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindblur="__e" bindinput="__e"/><view hidden="{{!(employ)}}" class="employ">已占用</view><image hidden="{{!(isuser)}}" class="ok" src="../../static/images/register/right.png"></image></view><view class="inputs-div"><input class="input" type="{{type}}" placeholder="请设置密码" placeholder-style="color:#aaa;font-weight:400;" data-event-opts="{{[['blur',[['check']]],['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindblur="__e" bindinput="__e"/><view hidden="{{!(employ)}}" class="employ">已占用</view><image class="show" src="{{showurl}}" data-event-opts="{{[['tap',[['showon']]]]}}" bindtap="__e"></image></view><view class="inputs-div"><input class="input" type="{{type}}" placeholder="请确认密码" placeholder-style="color:#aaa;font-weight:400;" data-event-opts="{{[['blur',[['checknumber']]],['input',[['__set_model',['','check_password','$event',[]]]]]]}}" value="{{check_password}}" bindblur="__e" bindinput="__e"/><view hidden="{{!(employ)}}" class="employ">已占用</view><text hidden="{{!(checkshow)}}" class="checknumber">两次密码不一致</text><image class="show" src="{{showurl}}" data-event-opts="{{[['tap',[['showon']]]]}}" bindtap="__e"></image></view></view></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit" bindtap="__e">注册</view></view>